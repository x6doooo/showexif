{
  "name": "showExif",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "show exif",
  "permissions": [
    "contextMenus"
  ],
  "browser_action": {
  	"default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery-1.8.2.js", "binaryajax.js", "exif.js", "showexif.js"]
    }
  ]
}
